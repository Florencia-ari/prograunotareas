Algoritmo eje2
	// Maria Florencia Ariaga Chiroy
	// 9959-23-6741
	Escribir 'Calcular el cociente y residuo de la division de dos numeros A y B';
	Definir A,B,Cociente,Residuo Como Real;
	Escribir 'Ingrese el valor de A:';
	Leer A;
	Escribir 'Ingrese el valor de B:';
	Leer B;
	Cociente <- A/B;
	Residuo <- A MOD B;
	Escribir 'Cociente: ',Cociente;
	Escribir 'Residuo: ',Residuo;
FinAlgoritmo
